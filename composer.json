{
    "name": "tobischulz/artisan-forge-deploy",
    "description": "A Laravel artisan command to easily deploy your code to a laravel forge server.",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Schulz",
            "email": "tobias@byte.software",
            "homepage": "https://byte.software"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "TobiSchulz\\ArtisanForgeDeploy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TobiSchulz\\ArtisanForgeDeploy\\ArtisanForgeProvider"
            ]
        }
    }
}
